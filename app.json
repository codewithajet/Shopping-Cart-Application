{
  "expo": {
    "name": "Kidaf PREMIUM SHOPPING",
    "slug": "Kidaf-premium-shopping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kidaf.png",
    "scheme": "shoppingcartapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kidaf.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.codewithajet.Kidafpremiumshopping"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/kidaf.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kidaf.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a4fa7f7-0d08-479e-a303-885d73934eea"
      }
    },
    "owner": "codewithajet"
  }
}
